{
  "dirTemplate": "{MYDOCS}\\VSDebugVariables",
  "newtonsoftJsonTemplate": "System.IO.File.WriteAllText({FILEPATH}, Newtonsoft.Json.JsonConvert.SerializeObject({VAR}, Newtonsoft.Json.Formatting.Indented, new Newtonsoft.Json.JsonSerializerSettings { ReferenceLoopHandling = Newtonsoft.Json.ReferenceLoopHandling.Ignore }))",
  "sysTextJsonTemplate": "System.IO.File.WriteAllText({FILEPATH}, System.Text.Json.JsonSerializer.Serialize({VAR}, new System.Text.Json.JsonSerializerOptions { WriteIndented = true }))",
  "utf8JsonTemplate": "System.IO.File.WriteAllText({FILEPATH}, Utf8Json.JsonSerializer.PrettyPrint(Utf8Json.JsonSerializer.Serialize({VAR}))"
}